<h1>Edit your article</h1>

<%= form_for(@article) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>

<p>
  <%= link_to articles_path do %>
    Back to articles
    <i class="fas fa-reply-all"></i>
  <% end %>
</p>
